{
  "name": "aqa-cypress",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "cypress-open": "npx cypress open",
    "cypress-open-dev": "npx cypress open --env configFile=dev",
    "cypress-open-prod": "npx cypress open --env configFile=prod",
    "cypress-run": "npx cypress run",
    "cypress-run-dev": "npx cypress run --env configFile=dev",
    "cypress-run-prod": "npx cypress run --env configFile=prod",
    "mochawesome-merge-json": "npx mochawesome-merge cypress/reports/*.json > cypress/reports/mochawesome/combined.json",
    "mochawesome-generate-html": "npx marge cypress/reports/mochawesome/combined.json --reportDir cypress/reports/mochawesome --inline",
    "test-with-report-dev": "npm run cypress-run-dev && npm run mochawesome-merge-json && npm run mochawesome-generate-html",
    "test-with-report-prod": "npm run cypress-run-prod && npm run mochawesome-merge-json && npm run mochawesome-generate-html"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "cypress": "^15.4.0",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^4.4.1",
    "mochawesome-report-generator": "^6.3.0"
  }
}
